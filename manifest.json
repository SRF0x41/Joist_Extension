{
  "manifest_version": 3,
  "name": "DOM Reader",
  "version": "1.0",
  "description": "Read the DOM of a webpage",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],

  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
